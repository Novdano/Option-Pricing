SABR_MC_Call <- function(S_0, K, r, alpha_0, beta, rho, nu, T, M, N){
payoff <- 0
for(i in 1:M){
S <- S_0
alpha <- alpha_0
for(n in 1:N){
W1 <- rnorm(1)
W2 <- rnorm(1)
S <- S + r*S*(T/N) + alpha*S^beta*sqrt(T/N)*W1*(exp((-r*(T-n*(T/N)))))^(1-beta)
alpha <- alpha + nu*alpha*(rho * W1 + sqrt(1-rho^2)*W2)*sqrt(T/N)
}
if (S>K){
payoff <- payoff + S-K
}
}
return ((payoff/M) * (1+r*(T/N))^-N)
}
